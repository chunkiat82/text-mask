{
  "name": "text-mask",
  "version": "0.0.2",
  "description": "To provide a utility container to deal with mask text and input",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "babel ./modules -d lib",
    "test": "npm run lint && npm run mocha",
    "lint": "node_modules/eslint/bin/eslint.js --ignore-path .gitignore modules/* examples/*",
    "compile": "babel -d lib/ modules/",
    "prepublish": "npm run compile",
    "mocha": "mocha --compilers js:mocha-babel test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chunkiat82/text-mask.git"
  },
  "keywords": [
    "mask",
    "input",
    "text"
  ],
  "author": "Raymond Ho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chunkiat82/text-mask/issues"
  },
  "homepage": "https://github.com/chunkiat82/text-mask",
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.4.0",
    "babel-runtime": "^5.8.24",
    "classnames": "^2.2.1",
    "eslint": "^1.10.1",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-react": "^3.10.0",
    "html-webpack-plugin": "^1.7.0",
    "mocha": "^2.3.4",
    "mocha-babel": "^3.0.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "webpack-dev-server": "^1.14.0"
  }
}
